# Daagent Configuration
# Copy this file to .env and fill in your API keys

# Primary Provider - OpenRouter (Free tier available)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Alternative Providers (Optional - leave empty if not using)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
TOGETHER_API_KEY=your_together_api_key_here
GEMINI_API_KEY=your_google_gemini_api_key_here
GROK_API_KEY=your_grok_api_key_here

# Legacy keys (for compatibility)
OPENAI_API_KEY=your_openai_api_key_here

# Model Selection (Optional - uses provider defaults if not set)
OPENROUTER_MODEL=nex-agi/deepseek-v3.1-nex-n1:free

# Agent Settings
MAX_ITERATIONS=25
TEMPERATURE=0.7
DEV_MODE=true

# MCP Warehouse Integration (Optional)
MCP_WAREHOUSE_PATH=C:\path\to\your\mcp\warehouse
ENABLE_MCP=true

# Future keys (leave empty)
# ANTHROPIC_API_KEY=your_anthropic_key_here
# DEEPSEEK_API_KEY=your_deepseek_key_here